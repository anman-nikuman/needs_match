<h4>イベント編集</h4>
<%= form_with model: @event, url: staffs_event_path, local: true do |f| %>
  <table>
    <tbody>
      <tr>
        <td>支部名</td>
        <td><%= @branch.name %></td>
      </tr>
      <tr>
        <td>開催日時</td>
        <td><%= f.datetime_select :date, minute_step: 30 %></td>
      </tr>
      <tr>
        <td>【会場】</td>
        <td></td>
      </tr>
      <tr>
        <td>最寄駅</td>
        <td><%= f.text_field :station1 %></td>
      </tr>
      <tr>
        <td>郵便番号</td>
        <td><%= f.text_field :postal_code %></td>
      </tr>
      <tr>
        <td>都道府県</td>
        <td><%= f.collection_select :prefecture_code, JpPrefecture::Prefecture.all, :code, :name %></td>
      </tr>
      <tr>
        <td>市町村以下</td>
        <td><%= f.text_field :address %></td>
      </tr>
    </tbody>
  </table>

  <%= f.submit "編集確定" %>
<% end %>
