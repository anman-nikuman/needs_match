<div class="container">
  <h4>検索結果：全<%= @event_seaches.count %>件</h4>
  <table class="table mt-5">
    <thead>
      <th></th>
      <th>　開催日時</th>
      <th>イベント名</th>
    </thead>
    <% @event_seaches.each do |e| %>
      <tbody>
        <tr>
          <td><%= l e.date %></td>
          <td><%= e.area %></td>
        </tr>
      </tbody>
    <% end %>
  </table>
  <%= paginate @event_seaches %>
</div>
